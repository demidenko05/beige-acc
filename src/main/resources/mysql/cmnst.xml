<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Beigesoft ORM common settings</comment>
<entry key="appInf">Beige-BLC ver1.0</entry>
<entry key="dbUrl">jdbc:mysql://localhost/beigeacc</entry>
<entry key="dsCls">com.mysql.jdbc.jdbc2.optional.MysqlDataSource</entry>
<entry key="dbUsr">beigeaccounting</entry>
