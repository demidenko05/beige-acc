<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Beigesoft Accounting</comment>
<entry key="appInf">Beige-ACC ver1.0</entry>
<entry key="jdbcCls">org.sqlite.JDBC</entry>
<entry key="dbUrl">jdbc:sqlite:#currentDir#bsa.sqlite</entry>
<entry key="checkTbl">select * from SQLITE_MASTER where TBL_NAME=':tblNm';</entry>
<entry key="lastId">select last_insert_rowid() as LASTID;</entry>
</properties>
